{
    "editor.fontFamily": "'Fira Code',  'Courier New', Consolas, monospace",
    "editor.fontSize": 16,
    "editor.lineHeight": 0,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.crswap": true,
        "go.mod": true,
        "go.sum": true,
    },
    "editor.tokenColorCustomizations": {
        "[Oceanside]": {
            "comments": "#aca6ad",
            "variables": "#ffaa00",
            "numbers": "#ff2222"
        },
        "[*Dark*]": {
            "variables": "#229977"
        },
        "[Abyss][Red]": {
            "keywords": "#f00"
        }
    }
}